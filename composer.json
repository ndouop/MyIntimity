{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "bosnadev/repositories": "dev-master",
        "calcinai/php-imagick": "^0.1.2",
        "doctrine/dbal": "~2.3",
        "highideas/laravel-users-online": "^2.2",
        "infyomlabs/adminlte-templates": "5.4.x-dev",
        "infyomlabs/laravel-generator": "5.4.x-dev",
        "infyomlabs/swagger-generator": "dev-master",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "jlapp/swaggervel": "dev-master",
        "landish/pagination": "^1.3",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "mcamara/laravel-localization": "^1.2",
        "paydunya/paydunya": "^1.0",
        "unisharp/laravel-filemanager": "^1.8",
        "yajra/laravel-datatables-buttons": "1.0",
        "yajra/laravel-datatables-oracle": "7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^1.1",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Supports/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
